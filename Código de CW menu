local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("CW", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("LOLL")
Section:NewButton("Auto parry", "Auto parry (must have good ping 70-100)", function()
local lp = game.Players.LocalPlayer
--inicio
local animationInfo = {}

function getInfo(id)
  local success, info = pcall(function()
      return game:GetService("MarketplaceService"):GetProductInfo(id)
  end)
  if success then
      return info
  end
  return {Name=''}
end
function block(player)
  keypress(0x46)
  wait()
  keyrelease(0x46)
end

local AnimNames = {
  'Slash',
  'Swing',
  'Sword'
}

function playerAdded(v)
    local function charadded(char)
      local humanoid = char:WaitForChild("Humanoid", 5)
      if humanoid then
          humanoid.AnimationPlayed:Connect(function(track)
              local info = animationInfo[track.Animation.AnimationId]
              if not info then
                  info = getInfo(tonumber(track.Animation.AnimationId:match("%d+")))
                  animationInfo[track.Animation.AnimationId] = info
              end
             
              if (lp.Character and lp.Character:FindFirstChild("Head") and v.Character:FindFirstChild("Head")) then
                  local mag = (v.Character.Head.Position - lp.Character.Head.Position).Magnitude
                  if mag < 15  then
                     
                      for _, animName in pairs(AnimNames) do
                          if info.Name:match(animName) then
                              pcall(block, v)
                          end
                      end
                     
                  end
              end
          end)
      end
  end
 
  if v.Character then
      charadded(v.Character)
  end
  v.CharacterAdded:Connect(charadded)
end

for i,v in pairs(game.Players:GetPlayers()) do
   if v ~= lp then
       playerAdded(v)
   end
end

game.Players.PlayerAdded:Connect(playerAdded)
end)
Section:NewButton("infinite stamina", "shes a runner shes a trackstar", function()
for i,v in pairs(getgc(true)) do
   if typeof(v) == "table" and rawget(v, "getIsMaxed") then
       v.getIsMaxed = function()
           return false
       end
       v.getFlags = function()
           return 1
       end
       v.addFlags = function(a,b)
           a:setFlags(0)
           return
       end
   end
   if typeof(v) == "table" and rawget(v, "spawnCharacter") then
       local oldfunc = v.spawnCharacter
       v.SpawnCharacter = function(a)
           for _,f in pairs(getgc(true)) do
               if typeof(f) == "table" and rawget(f, "getIsMaxed") then
                   f.getIsMaxed = function()
                       return false
                   end
                   f.getFlags = function()
                       return 1
                   end
                   f.addFlags = function(aa,b)
                       aa:setFlags(0)
                       return
                   end
               end
           end
       end
   end
   if typeof(v) == "table" and rawget(v, "getCanJump") then
       v.getCanJump = function()
           return true
       end
   end
   if typeof(v) == "table" and rawget(v, "JUMP_DELAY_ADD") then
       v.JUMP_DELAY_ADD = 0.5
   end
   if typeof(v) == "table" and rawget(v, "_setStamina") then
       v._setStamina = function(a, b)
           a._stamina = math.huge
           a._staminaChangedSignal:Fire(99)
       end
   end
end
game.StarterGui:SetCore("SendNotification", {Title = "CB", Text = "Script loaded, "..game.Players.LocalPlayer.DisplayName..".", Duration = 4,})
end)

Section:NewButton("hitbox", "credits Vuala", function()
_G.HeadSize = 20
_G.Disabled = true
game:GetService('RunService').RenderStepped:connect(function()
if _G.Disabled then
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
pcall(function()
v.Character.Head.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.Head.Transparency = 100
v.Character.Head.BrickColor = BrickColor.new("Red")
v.Character.Head.Material = "Neon"
v.Character.Head.CanCollide = false
v.Character.Head.Massless = true
end)
end
end
end
end)
end)
Section:NewButton("hitbox expander extra", "combinar con lo demas", function()
    loadstring(game:HttpGet("https://rawscripts.net/raw/Combat-Warriors-Hitbox-Expander-UPD2-16433"))()
end)
Section:NewButton("infiniX", "InfiniX menu", function()
    local __function = loadstring(game:HttpGet("https://github.com/Hosvile/InfiniX/releases/latest/download/main.lua", true))

__function(__function)
end)
Section:NewButton("Auto equip DS", "No es el auto equip de mi menu normal", function()
    -- LocalScript

local player = game.Players.LocalPlayer

-- Nombre de la herramienta que quieres que se auto equipe
local mainToolName = "Dragon Slayer"

local function equipMainTool()
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player:WaitForChild("Backpack")

    -- Desequipa la herramienta principal si hay alguna otra herramienta equipada
    local equippedTool = character:FindFirstChildOfClass("Tool")
    if equippedTool and equippedTool.Name ~= mainToolName then
        -- Si hay una herramienta equipada que no sea la principal, no hacer nada
        return
    end
    
    -- Desequipa la herramienta principal si hay alguna otra herramienta equipada
    for _, tool in ipairs(character:GetChildren()) do
        if tool:IsA("Tool") and tool.Name ~= mainToolName then
            tool.Parent = backpack
        end
    end
    
    -- Busca la herramienta principal en la mochila
    local mainTool = backpack:FindFirstChild(mainToolName)
    
    -- Equipa la herramienta principal si no est치 equipada
    if mainTool and not character:FindFirstChild(mainToolName) then
        mainTool.Parent = character
    end
end

-- Equipar la herramienta principal cuando el personaje se a침ade
player.CharacterAdded:Connect(function()
    equipMainTool()
end)

-- Equipar la herramienta principal inmediatamente si el personaje ya est치 cargado
if player.Character then
    equipMainTool()
end

-- Verificar constantemente si la herramienta principal est치 equipada
game:GetService("RunService").Heartbeat:Connect(function()
    equipMainTool()
end)

-- Asegurarse de que la herramienta principal se mantenga equipada cuando se desequipa
player.Backpack.ChildAdded:Connect(function(child)
    if child:IsA("Tool") and not player.Character:FindFirstChildOfClass("Tool") then
        equipMainTool()
    end
end)
end)